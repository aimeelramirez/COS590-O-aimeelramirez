{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  },
  "types": ["react", "react-native", "@types/jest"],
  "paths": {
    "@react-navigation/*": ["node_modules/@react-navigation/*"]
  }
}
